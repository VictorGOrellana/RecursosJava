<body>
	<h3>Pagina Pai Load Jquery</h3>
		<input type="button" value="Carregar Pag. Filha" onclick="carregar();">
		
		
		<div id="mostrarPaginaFilha"></div>
		

</body>
<script>
	function carregar(){
		$("mostrarPaginaFilha").load('paginaFilha.jsp');
		
	}
	

</script>
